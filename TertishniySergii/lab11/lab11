class Animal:
    def sound(self):
        pass

class Cat(Animal):
    def sound(self):
        return "Meow"


class Dog(Animal):
    def sound(self):
        return "Woof"

#Factory Method
class AnimalFactory:
    def create_animal(self, animal_type):
        if animal_type == "cat":
            return Cat()
        elif animal_type == "dog":
            return Dog()
        else:
            return None

factory = AnimalFactory()

animal1 = factory.create_animal("cat")
animal2 = factory.create_animal("dog")
print(animal1.sound())  
print(animal2.sound())  
